<p id="notice"><%= notice %></p>

<h1>Blogcounts</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Title</th>
      <th>Options</th>
    </tr>
  </thead>

  <tbody>
    <% @blogcounts.each do |blogcount| %>
      <tr>
        <td><%= link_to blogcount.title, blogcount %></td>

        <td>
        <%= link_to 'Edit', edit_blogcount_path(blogcount) %>
        <%= link_to 'Delete', blogcount, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Blog', new_blogcount_path %>
